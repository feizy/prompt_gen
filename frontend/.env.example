# API Configuration
REACT_APP_API_URL=http://localhost:8000/v1
REACT_APP_WS_URL=ws://localhost:8000/ws

# Application Configuration
REACT_APP_NAME=AI Agent Prompt Generator
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION=Create detailed LLM prompts through collaborative AI agents

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG=true

# WebSocket Configuration
REACT_APP_WS_RECONNECT_ATTEMPTS=5
REACT_APP_WS_RECONNECT_DELAY=1000

# UI Configuration
REACT_APP_DEFAULT_THEME=light
REACT_APP_SIDEBAR_DEFAULT_OPEN=true

# Performance
REACT_APP_MESSAGE_LIMIT=100
REACT_APP_SESSION_LIMIT=50

# Timeouts (in milliseconds)
REACT_APP_API_TIMEOUT=30000
REACT_APP_WS_TIMEOUT=5000